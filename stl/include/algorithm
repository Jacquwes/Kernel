#pragma once

#include <string>

namespace std
{
	template <typename InputIt1, typename InputIt2>
	constexpr bool equal(InputIt1 first1, InputIt1 last, InputIt2 first2);

	template <typename InputIt1, typename InputIt2>
	bool lexicographical_compare(InputIt1 first1, InputIt1 last1,
								 InputIt2 first2, InputIt2 last2);
	
}